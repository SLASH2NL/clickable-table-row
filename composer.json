{
    "name": "digital-creative/clickable-table-row",
    "description": "Makes every Nova table resource rows clickable",
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "table",
        "clickable",
        "row"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\ClickableTableRow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\ClickableTableRow\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
